{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:4173/NotesReview/"
      ],
      "startServerCommand": "npm run serve",
      "startServerReadyPattern": "running|listen|ready|local",
      "numberOfRuns": 3,
      "settings": {
        "chromeFlags": "--no-sandbox",
        "preset": "desktop"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
